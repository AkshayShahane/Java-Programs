
import java.util.ArrayList;
/*This is ArrayList Program to create,add elements and print the size of arraylist*/
public class ArrayList2 
{
	public static void main(String[] args)
	{
		ArrayList al=new ArrayList();
		al.add(10);
		al.add(98);
		al.add(10);
		al.add(45);
		al.add(20);
		al.add(77);
		al.add(84);
		al.add(52);
		System.out.println(al.size());
	}
}
