
/*WAP to print the even numbers from 1 to 100 in an increasing order*/
public class s1 
{
	public void meth1()
	{
		System.out.println("****Even Numbers***");
		System.out.println("============================");
		for(int i=1;i<=100;i++)
		{
			if(i%2==0)
			{
				System.out.println(i);
			}
		}
	}
	public static void main(String[] args) 
	{
		s1 obj=new s1();
		obj.meth1();
	}
}
