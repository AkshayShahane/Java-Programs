//program to print elements which is on even position
public class array_even_elements 
{
	public static void main(String[] args) 
	{
		int arr[]= {12,20,55,66,88,55,11,90,122};
		System.out.println("Elements in the even position of an array");
		for(int i=0;i<arr.length;i++)
		{
			if(i%2==0)
			{
				System.out.println(arr[i]);
			}
		}
	}
}
