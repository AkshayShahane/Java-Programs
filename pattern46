
/*
5 6 15 16 25
4 7 14 17 24
3 8 13 18 23
2 9 12 19 22
1 10 11 20 21

*/
public class pp28 
{
	public static void main(String[] args) 
	{
		int k=5,m=6,p,q;
		for(int i=5;i>=1;i--)
		{
			p=k;
			q=m;
			for(int j=1;j<=5;j++)
			{
				if(j%2==0)
				{
					System.out.print(" "+q);
					q=q+10;
				}
				else
				{
					System.out.print(" "+p);
					p=p+10;
				}
				
			}
			k--;
			m++;
			System.out.println();
		}
	}
}
