
public class Demo 
{
	Demo()
	{
		this(50);
		System.out.println("non-parameterized constructor");
	}
	Demo(int a)
	{
		this("Java is awesome");
		System.out.println(a);
	}
	Demo(String s)
	{
		this(10,20);
		System.out.println(s);
	}
	Demo(int a,int b)
	{
		int c;
		c=a+b;
		System.out.println(c);
	}
	
	public static void main(String[] args) 
	{
		new Demo();
		
	}
}
