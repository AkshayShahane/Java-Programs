
public class six 
{
	void meth1()
	{
		for(int i=5;i>=1;i--)
		{
			for(int j=1;j<=5;j++)
		 	{
				if(j<=5)
				{
					System.out.print(i+" ");
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println(" ");
		}
	}
	public static void main(String[] args) 
	{
		new six().meth1();
	}
}
