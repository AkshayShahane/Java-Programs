
/*
1  3  5  7  9
3  5  7  9 11
5  7  9 11 13
7  9 11 13 15
9 11 13 15 17
*/


public class pp24
{
	public static void main(String[] args) 
	{
		int k=0,m;
		for(int i=1;i<=5;i++)
		{
			m=k+i;
			for(int j=1;j<=5;j++)
			{
				if(j<=5)
				{
					if(j>1)
					{
						
						System.out.printf(" %2d",m+2);
						m=m+2;
					}
					else
					{
						System.out.print(" "+m);
					}
				}
			}
			k++;
			System.out.println();
		}
	}
}
