
/*WAP of continue statement in which iterate the numbers from 1 to 10 and
 * skip the number 5 and print reamaining numbers. */

public class continueEg 
{
	void meth1()
	{
		for(int i=1;i<=10;i++)
		{
			if(i==5)
			{
				continue;
			}
			else
			{
				System.out.println("i===> "+i);
			}
		}
	}
	
	public static void main(String[] args)
	{
		new continueEg().meth1();
	}
}
