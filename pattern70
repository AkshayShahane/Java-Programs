class pp48
{
	public static void main(String[] args) 
	{
		int k;
		for(int i=1;i<=5;i++)
		{
			k=i;
			for(int j=1;j<=5;j++)
			{
				if(j>=6-i)
				{
					if(i%2==0)
					{
						System.out.print(k);
						k=k+i;
					}
					else
					{
						System.out.print(k);
						k=k+i;
					}
				}
				else
				{
					System.out.print(" ");
				}
			}
			
			System.out.println();
		
		}
	}
}
