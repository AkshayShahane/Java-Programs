
/*
55555
5555 
555  
55   
5
*/
public class pp33 
{
	public static void main(String[] args)
	{
		int k;
		for(int i=1;i<=5;i++)
		{
			k=5;
			for(int j=1;j<=5;j++)
			{
				if(j<=6-i)
				{
					System.out.print(k);
				}
				else
				{
					System.out.print(" ");
				}
			}
			k--;
			System.out.println();
		}
	}
}
