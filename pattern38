
/*
1 1 2 1 3 1
1 2 2 2 3 2
1 3 2 3 3 3
1 4 2 4 3 4
1 5 2 5 3 5
*/


public class pp21 
{
	public static void main(String[] args)
	{
		int k;
		for(int i=1;i<=5;i++)
		{
			k=1;
			for(int j=1;j<=6;j++)
			{
				if(j<=6)
				{
					if(j%2==0)
					{
						System.out.print(" "+i);
					}
					else
					{
						System.out.print(" "+k);
						k++;
					}
				}
			}
			System.out.println();
		}
	}
}
