/*

1
3 3 3
5 5 5 5 5 5
3 3 3
1

*/

public class test
{
	public static void main(String[] args)
	{
		int k=0,m=4,n;
		for(int i=1;i<=7;i++)
		{
			
			if(i<=4)
			{
				k++;
				m--;
			}
			else
			{
				k--;
				m++;
			}
			n=m;
			for(int j=1;j<=4;j++)
			{
				
				if(j<=k)
				{
					System.out.print(n);
					n++;
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}
