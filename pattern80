
/*
A
BA
CBA
DCBA
EDCBA
*/

public class pp52
{
	public static void main(String[] args)
	{
		char k;
		for(int i=1;i<=5;i++)
		{
			//k=(char)(64+i);
			
			for(int j=1;j<=5;j++)
			{
				if(j>=6-i)
				{
					System.out.print(k);
					k--;
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}
