/*

1
3 3 3
5 5 5 5 5 5
3 3 3
1

*/

public class test
{
	public static void main(String[] args)
	{
		int k=1 ;
		for(int i=1;i<=5;i++)
		{
			for(int j=1;j<=5;j++)
			{
				if(j<=k)
				{
					System.out.print(k);
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
			if(i<3)
			  k=k+2;
			else
			  k=k-2;
		}
	}
}
