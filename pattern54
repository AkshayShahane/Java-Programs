/*
54321
5432 
543  
54   
5
*/
public class pp35 
{
	public static void main(String[] args) 
	{
		int k;
		for(int i=1;i<=5;i++)
		{
			k=5;
			for(int j=1;j<=5;j++)
			{
				if(j<=6-i)
				{
					System.out.print(k);
					k--;
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}
