
/* WAP to print the following numbers in reverse order.
34,56,54,32,67,89,90,32,21
 */
class arrrev 
{
	
	public void display()
	{
		int[] arr= {34,56,54,32,67,89,90,32,21};
		
		for(int i=0;i<=8;i++)
		{
			System.out.println(arr[i]);
		}
		System.out.println("----------------------");
		
		for(int i=arr.length-1;i>=0;i--)
		{
			System.out.println(arr[i]);
		}
	}
	
	public static void main(String[] args) 
	{
		new arrrev().display();
	}
}
