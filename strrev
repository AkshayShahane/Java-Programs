// program to print reverse a string
public class test6
{
	
	static String str="Palindrome";
	
	void meth1()
	{
		for(int i=str.length()-1;i>=0;i--)
		{
			System.out.print(str.charAt(i));
		}
	}
	public static void main(String[] args) 
	{
		System.out.println("Original String is ==>"+str);
		test6 obj=new test6();
		obj.meth1();
	}
}
