/*
1
21
321
4321
54321
*/
class pp46 
{
	public static void main(String[] args) 
	{
		int k;
		for(int i=1;i<=5;i++)
		{
			k=i;
			for(int j=1;j<=5;j++)
			{
				if(j>=6-i)
				{
					System.out.print(k);
					k--;
				}
				else
				{
					System.out.print(" ");
				}
			}
			
			System.out.println();
		
		}
	}
}
