
public class exceptionhandling2
{
	private void exceptionHandlingMethod()
	{
		String s=null;
		
		try
		{
			System.out.println("try block exceuting...");
			System.out.println(s.length());
			System.out.println("try block executed");
		}
		catch(NullPointerException e)
		{
			System.out.println("Sir/Mam I catched Exception");
			System.out.println("Please provide appropriate Data: ");
		}
		finally
		{
			System.out.println("finally block executed..");
		}
	}
	public static void main(String[] args) 
	{
		new exceptionhandling2().exceptionHandlingMethod();
	}
}
