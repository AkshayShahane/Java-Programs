/*WAP to count no of digits in a number*/

public class DigitCountFromNumber 
{
	int num=567888888,count;
	
	public void meth1()
	{
		while(num!=0)
		{
			num=num/10;
			count++;
			
		}
		System.out.println("The total number of digits in number is==>"+count);
	}
	public static void main(String[] args) 
	{
		DigitCountFromNumber obj=new DigitCountFromNumber();
		obj.meth1();
	}
}
