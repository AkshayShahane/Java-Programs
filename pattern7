
public class seventh 
{
	void meth1()
	{
		for(int i=1;i<=5;i++)
		{
			for(int j=5;j>=1;j--)
		 	{
				if(j<=5)
				{
					System.out.print(j+" ");
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println(" ");
		}
	}
	public static void main(String[] args) 
	{
		new seventh().meth1();
	}
}
