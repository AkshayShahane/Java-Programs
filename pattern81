/*
E
DE
CDE
BCDE
ABCDE
*/

public class pp53 
{
	
	    public static void main(String[] args) 
	    {
	    	char k;
	    	for(int i=1;i<=5;i++)
	    	{
	    		k='A';
	    		for(int j=1;j<=5;j++)
	    		{
	    			
	    			if(j>=6-i)
	    			{
	    				System.out.print(k);
	    				k++;
	    			}
	    			else
	    			{
	    				System.out.print(" ");
	    				k++;
	    			}
	    		}
	    		System.out.println();
	    	}
		}
}
