//program to print the odd elements in an array
public class array_odd_elements 
{
	public static void main(String[] args) 
	{
		int arr1[]= {12,10,55,30,55,7,5,9};
		
		System.out.println("Printing odd elements in an array");
		for(int i=0;i<arr1.length;i++)
		{
			if(i%2!=0)
			{
				System.out.println(arr1[i]);
			}
		}
	}
}
