public class pp26
{
	public static void main(String [] akshay)
	{
		int k=1;
		for(int i=1;i<=5;i++)
		{
			for(int j=1;j<=5;j++)
			{
				System.out.print(" "+k);
				if(k==1)
				  k=0;
				else
				  k=1;
			}
			System.out.println();
		}
	}

}
