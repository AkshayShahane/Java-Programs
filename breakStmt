
import java.util.Scanner;
public class breakStmt 
{
	void meth1()
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter Number for loop iterration");
		int num=sc.nextInt();
		System.out.println("Enter Number to break the loop");
		int num2=sc.nextInt();
		
		for(int i=1;i<=num;i++)
		{
			if(i==num2)
			{
				break;
				
			}
			else
			{
				System.out.println("i==>"+i);
			}
		}
	}
	public static void main(String[] args) 
	{
		new breakStmt().meth1();
	}
}
