public class test
{
	public static void main(String[] kishan_sir)
	{
		char k='E';
		for(int i=1;i<=5;i++)
		{
			for(int j=1;j<=9;j++)
			{
				if(j>=i && j<=10-i)
				{
					if(i%2!=0 && j%2!=0)
					{
						System.out.print(k);
					}
					else if(i%2==0 && j%2==0)
					{
						System.out.print(k);
					}
					else
					{
						System.out.print(" ");
					}
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
			k--;
		}
	}
}
