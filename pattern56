/*
5 6 7 8 9 
4 5 6 7  
3 4 5   
2 3    
1
*/
public class pp37
{
	public static void main(String[] args)
	{
		int k;
		for(int i=1;i<=5;i++)
		{
			k=6-i;
			for(int j=1;j<=5;j++)
			{
				if(j<=6-i)
				{
					System.out.print(k+" ");
					k++;
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}
